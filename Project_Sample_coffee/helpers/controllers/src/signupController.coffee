angular.module('myApp').controller 'signupController', ($scope, $state, $location, authenticateService, sessionService) ->

  $scope.submitClick = ->
    if $scope.userInfo.$invlid == true
      alert 'some Invlid Information'
    else
      #authenticateService.logout();
      $location.path 'signin'
    return

  $scope.isfavoritesSelected = (object) ->
    Checked = false
    angular.forEach object, (value, key) ->
      if value.checked
        Checked = true
      return
    Checked

