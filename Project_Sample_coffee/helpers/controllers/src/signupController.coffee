      Text: 'Female'
    }
    {
      Id: 'O'
      Text: 'Other'
    }
  ]

  $scope.favoriteSelected = (position, favorites) ->
    angular.forEach favorites, (item, index) ->
      if position != index
        item.checked = false
      return
    return

  return

